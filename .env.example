# Server
PORT=3002

# Auth (must match backend, use openssl rand -hex 32 to generate)
HMAC_SECRET=

# API keys
OPENROUTER_API_KEY=
OPENAI_API_KEY=

# Timeouts
CONNECT_TIMEOUT=10000
REQUEST_TIMEOUT=1000000

# Rate limiting / concurrency
RATE_LIMIT_PER_USER=30
GLOBAL_CONCURRENCY=10

# Retries
MAX_RETRIES=2
RETRY_BACKOFF_MS=1000

# Logging
LOG_LEVEL=info
